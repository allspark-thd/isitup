{
  "name": "IsItUp",
  "version": "0.0.1",
  "description": "Simple way to see if things are up",
  "author": "gambtho <thomas_gamble@homedepot.com>",
  "main": "index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "npm run server:dev",
    "start:hmr": "npm run server:dev -- --env.HMR",
    "server:dev": "node_modules/webpack-dev-server/bin/webpack-dev-server.js --env.ENV development",
    "build": "rimraf dist && node_modules/webpack/bin/webpack.js --env.ENV development",
    "typings-install": "node_modules/typings/dist/bin.js install",
    "pretest": "npm install",
    "clean": "rimraf node_modules doc dist && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean && npm start",
    "watch": "webpack --watch --progress --profile",
    "server": "webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 8081 --content-base app",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "e2e": "protractor",
    "e2e-live": "protractor --elementExplorer",
    "postinstall": "npm run webdriver-update && typings install",
    "prestart": "npm install",
    "test-watch": "karma start test/karma.conf.js",
    "test": "npm run build && karma start test/karma.conf.js  --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor test/protractor-conf.js"
  },
  "dependencies": {
    "@angular2-material/card": "^2.0.0-alpha.3",
    "@angular2-material/core": "^2.0.0-alpha.3",
    "@angular2-material/input": "^2.0.0-alpha.3",
    "angular2": "2.0.0-beta.16",
    "rxjs": "5.0.0-beta.7",
    "zone.js": "~0.6.12"
  },
  "devDependencies": {
    "angular2-rest": "^0.1.0",
    "awesome-typescript-loader": "~v0.17.0",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.35.0",
    "json-loader": "^0.5.4",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "0.1.3",
    "rimraf": "^2.5.2",
    "ts-node": "^0.7.2",
    "typescript": "~1.8.10",
    "typings": "~0.8.1",
    "webpack": "~2.1.0-beta.5",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "v2.0.0-beta"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/allspark-thd/isitup/issues"
  },
  "homepage": "https://github.com/allspark-thd/isitup#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/allspark-thd/isitup.git"
  }
}
